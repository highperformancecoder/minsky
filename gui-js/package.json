{
  "name": "minsky",
  "version":"3.1.0-beta.7",
  "author": "High Performance Coders",
  "description": "Graphical dynamical systems simulator oriented towards economics",
  "build": {
    "beforePack": "beforePackHook.js",
    "artifactName": "${productName}-${version}.${ext}",
    "mac": {
      "icon": "../minsky.app/Contents/Resources/MinskyLogo.icns"
    },
    "win": {
      "icon": "apps/minsky-electron/src/assets/MinskyLogo.ico"
    },
    "linux": {
      "category": "Science"
    },
    "extraFiles": [{"from": "dynamic_libraries", "to": ""}],
    "fileAssociations": [
      {
        "ext": "mky",
        "name": "Minsky",
        "icon": "apps/minsky-electron/src/assets/MinskyLogo"
      },
      {
        "ext": "rvl",
        "name": "Ravel",
        "icon": "apps/minsky-electron/src/assets/RavelLogo"
      }
    ]
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && exitzero electron-builder install-app-deps",
    "nx": "nx",
    "start": "npm-run-all -p web electron",
    "start:log": "DEBUG=minsky:* DEBUG_COLOR=true npm run start",
    "web": "nx serve",
    "electron": "wait-on http-get://localhost:4200/ && nx serve minsky-electron",
    "build:web": "nx build minsky-web --prod",
    "build:electron": "nx build minsky-electron --prod",
    "export:package:windows": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform windows",
    "export:package:linux": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform linux --arch x64 --publishPolicy never",
    "export:package:mac": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform mac",
    "doc": "npx compodoc -p tsconfig.compodoc.json -n \"Minsky\"",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator",
    "bump:major": "nx run workspace:version --version=major",
    "bump:minor": "nx run workspace:version --version=minor",
    "bump:patch": "nx run workspace:version --version=patch",
    "bump:alpha": "nx run workspace:version --version=prerelease --preid=alpha",
    "bump:beta": "nx run workspace:version --version=prerelease --preid=beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.1",
    "@angular/cdk": "12.0.1",
    "@angular/common": "12.0.1",
    "@angular/compiler": "12.0.1",
    "@angular/core": "12.0.1",
    "@angular/forms": "12.0.1",
    "@angular/material": "12.0.1",
    "@angular/platform-browser": "12.0.1",
    "@angular/platform-browser-dynamic": "12.0.1",
    "@angular/router": "12.0.1",
    "@nrwl/angular": "^12.10.0",
    "@nrwl/linter": "12.3.4",
    "@nrwl/tao": "12.3.4",
    "ansi-html": "^0.0.9",
    "ansi-regex": "^6.0.1",
    "bindings": "^1.5.0",
    "debug": "^4.3.1",
    "electron-log": "^4.4.1",
    "electron-store": "^7.0.3",
    "is-electron": "^2.2.0",
    "json5": "^2.2.0",
    "JSONStream": "^1.3.5",
    "ngx-auto-unsubscribe": "^3.0.1",
    "ngx-color-picker": "^11.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.1.0",
    "utf8": "^3.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.1",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "12.0.1",
    "@angular/compiler-cli": "12.0.0",
    "@angular/language-service": "12.0.1",
    "@compodoc/compodoc": "^1.1.15",
    "@jscutlery/semver": "^2.10.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@nrwl/cli": "12.3.4",
    "@nrwl/cypress": "^12.10.0",
    "@nrwl/eslint-plugin-nx": "12.3.4",
    "@nrwl/jest": "12.3.4",
    "@nrwl/node": "12.3.4",
    "@nrwl/workspace": "12.3.4",
    "@twittwer/compodoc": "1.5.3",
    "@types/jest": "26.0.23",
    "@types/keysym": "0.0.1",
    "@types/node": "14.14.33",
    "@types/utf8": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "electron": "^18.3.7",
    "eslint": "7.18.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "exitzero": "^1.0.1",
    "husky": "^4.3.8",
    "jest": "^26.2.2",
    "jest-preset-angular": "8.4.0",
    "nx-electron": "11.3.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "4.2.4",
    "wait-on": "^5.3.0"
  },
  "engines": {
    "node": "14",
    "vscode": "^1.50.0",
    "npm": "7"
  }
}
